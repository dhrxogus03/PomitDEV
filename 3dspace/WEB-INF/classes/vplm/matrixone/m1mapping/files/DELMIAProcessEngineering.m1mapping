<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://www.3ds.com/enovia-matrixone/vplm-m1/mapping" version="9">

  <!-- Package -->
  <pattern.VPLM_PACKAGE>
    <vplm.Package name="DELMIAProcessEngineering" />
    <m1.package name="DELMIAProcessEngineering" />

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELArrowDisplayExt" />
      <m1.interface name="DELArrowDisplayExt" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ArrowEndPosition" />
        <m1.attribute name="DELArrowDisplayExt.V_ArrowEndPosition" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ArrowInternalData" />
        <m1.attribute name="DELArrowDisplayExt.V_ArrowInternalData" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_ArrowStartPosition" />
        <m1.attribute name="DELArrowDisplayExt.V_ArrowStartPosition" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

    <!-- Extension -->
    <pattern.VPLM_EXTENSION>
      <vplm.Extension name="DELDistributionTimeExt" />
      <m1.interface name="DELDistributionTimeExt" />

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_DistributionMode" />
        <m1.attribute name="DELDistributionTimeExt.V_DistributionMode" local="yes" />
      </pattern.VPLM_SIMPLE>

      <!-- Simple -->
      <pattern.VPLM_SIMPLE>
        <vplm.Simple name="V_DistributionParameters" />
        <m1.attribute name="DELDistributionTimeExt.V_DistributionParameters" local="yes" />
      </pattern.VPLM_SIMPLE>

    </pattern.VPLM_EXTENSION>

  </pattern.VPLM_PACKAGE>

</mapping>
