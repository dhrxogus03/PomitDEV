	<!-- ======================================================================================== -->
	<xsd:element name="XPDMXML" type="XPDMXMLType"/>
	<xsd:complexType name="XPDMXMLType">
		<xsd:annotation>
			<xsd:documentation>XPDMXML main Container</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="InfoHeader" type="InfoHeaderType" minOccurs="0"/>
			<xsd:element ref="XPDMXML_Structure" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="File" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="ListOfObjectBase" minOccurs="0"/>
			<xsd:element ref="ListOfAcknowledgementObjectBase" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="schemaVersion" type="xsd:positiveInteger" use="required" fixed="2"/>
	</xsd:complexType>
	<xsd:element name="MultiStructure" type="MultiStructureType" substitutionGroup="XPDMXML_Structure"/>
	<xsd:complexType name="MultiStructureType">
		<xsd:complexContent>
			<xsd:extension base="StructureBase">
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element ref="AnyDocumentBase" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="AnyDocumentSpecificationBase" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="AnyDocumentRelationshipBase" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="Parameter" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ParameterRelationship" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ManufacturingContext" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="SimulationContext" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="PCBContext" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
