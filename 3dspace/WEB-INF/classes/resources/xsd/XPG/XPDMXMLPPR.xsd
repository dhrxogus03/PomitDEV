  <xsd:complexType name="TransformationInstanceType">
    <xsd:annotation>
      <xsd:documentation>
        Product Transformation Inst gives a definition of a type of transformation to achieve (i.e. Intent)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="InstanceBase">
        <xsd:sequence>
          <xsd:choice minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
                If no attribute is defined, neither DiscreteQuantity nor UsageCoefficient, default behavior is DiscreteQuantity=1.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:element name="DiscreteQuantity" type="AttributeValuePositiveIntegerType"/>
            <xsd:element name="UsageCoefficient">
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:restriction base="AttributeValueDoubleType">
                    <xsd:minInclusive value="0"/>
                  </xsd:restriction>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:choice>
		  <xsd:element name="IsConstQuantity" type="AttributeValueBooleanType" minOccurs="0" maxOccurs="1"/>
          <xsd:group ref="ContinuousQuantityGroup" minOccurs="0" maxOccurs="1"/>
          <xsd:group ref="ProductImplementLinkRefGroup" minOccurs="0"/>
          <xsd:element name="IsPredecessorOfOwned" type="AttributeValueBooleanType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
                If IsPredecessorOfOwned is not defined, default behavior is IsPredecessorOfOwned=true.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:group ref="ReplaceStatusGroup" minOccurs="1" maxOccurs="1"/>
          <xsd:group ref="UseCaseGroup" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="IsFlexiblePart" type="AttributeValueBooleanType" minOccurs="0" maxOccurs="1"/>
          <xsd:group ref="FastenerCategoryGroup" minOccurs="0"/>
		  <xsd:group ref="DELFmiLossQuantityRatioGroup" minOccurs="0"/>
		  <xsd:group ref="DELFmiLimitQuantityGroup" minOccurs="0"/>
		  <xsd:group ref="RatioOfDiscreteQuantityGroup" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
