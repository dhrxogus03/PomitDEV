  <xsd:group name="NumberRequiredGroup">
    <xsd:sequence>
      <xsd:element name="IsSerialNumberRequired" type="AttributeValueBooleanType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specified if a serial number is required for the Unit correspoding to this Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IsLotNumberRequired" type="AttributeValueBooleanType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specified if a lot number is required for the Unit correspoding to this Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  <!--  -->
  <xsd:group name="NeedDedicatedSystemGroup">
    <xsd:sequence>
      <xsd:element name="NeedDedicatedSystem" type="Yes_No_Enum" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Describes whether a manufacturing system is expected for this Transformation or not</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="MakeOrBuyGroup">
    <xsd:sequence>
      <xsd:element name="MakeOrBuy" type="Make_Buy_Enum" default="Unassigned" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Describes if a manufacturing item is bought or made. Default is unassigned.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  <xsd:group name="MBOMGroup">
    <xsd:sequence>
      <xsd:element name="EstimatedCost" type="AttributeValueDoubleType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Defines the estimated cost of the Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EstimatedCostCurrency" type="AttributeValueStringType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Defines the money in which the Estimated cost is defined.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EstimatedLeadTimeDescription" type="AttributeValueStringType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Defines how long it takes for the Manufacturing Item to arrive once it is ordered.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ManufacturedItemClassification" type="AttributeValueStringType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Classifies the Manufacturing Item (example:Wiring Harness, Software, Sheet Metal, Machined...).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpareManufacturedItem" type="AttributeValueBooleanType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Is the Manufacturing Item a spare Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MaterialCategory" type="AttributeValueStringType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specifies the material category of the Manufacturing Item (example:Metal, Plastic...).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EstimatedWeight" type="MassType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specifies the estimated weight of the Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TargetReleaseDate" type="AttributeValueDateTimeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specifies the estimated weight of the Manufacturing Item.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:group>
  <!--  -->
  <xsd:group name="ContinuousQuantityGroup">
