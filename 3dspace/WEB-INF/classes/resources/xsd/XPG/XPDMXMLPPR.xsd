        <xsd:sequence>
          <xsd:element name="ResourceRef" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded" info:typeRef="ResourceBase"/>
          <xsd:element name="Documents" type="xsd:IDREF" info:typeRef="DocumentType" minOccurs="0"/>
          <xsd:element name="ResourcesQuantity" type="AttributeValueDoubleType" minOccurs="0"  maxOccurs="1"/>
          <xsd:element name="Priority" type="AttributeValuePositiveIntegerType" minOccurs="0"  maxOccurs="1"/>
          <xsd:element name="ManuallyScheduled" type="AttributeValueBooleanType" minOccurs="0"  maxOccurs="1"/>
          <xsd:element name="ToolChangeoverTime" type="AttributeValueDurationType" minOccurs="0"  maxOccurs="1"/>
          <xsd:element name="GroupIndex" type="AttributeValuePositiveIntegerType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="IsPreferred" type="AttributeValueBooleanType" minOccurs="1" maxOccurs="1"/>
          <xsd:element name="EstimatedTime" type="AttributeValueDurationType" minOccurs="0"/>
          <xsd:group ref="QuantityToBeProducedPerTimeGroup" minOccurs="0"  maxOccurs="1"/>
