          </xsd:annotation>
        </xsd:element>
        <xsd:element name="MassQuantity" type="MassType">
          <xsd:annotation>
            <xsd:documentation>Defines Mass Consumed</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
  </xsd:group>
