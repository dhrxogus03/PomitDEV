          <xsd:element name="PreAssignedSourceRef" type="ObjectPathRef" info:typeRef="ProductionSystemInstanceType ProductionOperationInstanceType"/>
          <xsd:element name="Documents" type="xsd:IDREF" info:typeRef="DocumentType" minOccurs="0"/>
          <xsd:element name="Duration" type="AttributeValueDurationType" minOccurs="0"/>
          <xsd:element name="Priority" type="AttributeValuePositiveIntegerType" minOccurs="0"/>
          <xsd:element name="ManuallyScheduled" type="AttributeValueBooleanType" minOccurs="0"/>
          <xsd:element name="ResourcesQuantity" type="AttributeValueDoubleType" minOccurs="0"/>
          <xsd:element name="EstimatedTime" type="AttributeValueDurationType" minOccurs="0"/>
          <xsd:group ref="QuantityToBeProducedPerTimeGroup" minOccurs="0"/>
