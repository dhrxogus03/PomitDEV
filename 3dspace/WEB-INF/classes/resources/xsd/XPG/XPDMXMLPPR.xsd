  <xsd:complexType name="ManufacturingContextType">
    <xsd:annotation>
      <xsd:documentation>
        ManufacturingContextType exposes one and only one CATProcess.
        It is not instantiated by any component.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="RepresentationBase"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ManufacturingContext" type="ManufacturingContextType"/>
  <!--    -->
  <xsd:complexType name="PortOnTransformationInstanceType">
    <xsd:complexContent>
      <xsd:extension base="ObjectBase">
        <xsd:sequence>
          <xsd:element name="Owned" type="xsd:IDREF" info:typeRef="TransformationInstanceType"/>
          <xsd:element name="PortRef" type="xsd:IDREF" info:typeRef="DataRequirementPortBase"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
