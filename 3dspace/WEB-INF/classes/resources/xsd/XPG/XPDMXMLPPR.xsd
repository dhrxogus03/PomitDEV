  <!--  -->
  <xsd:complexType name="DataCollectPlanRowType">
    <xsd:annotation>
      <xsd:documentation>
        A Data Collect Plan Row has some required attributes:
        - a collect mode
        - a number of measurements greater or equal to 1
        - a type (Text, Integer, Boolean, Timestamp, LENGTH, MASS, etc.)
        The Data Collect Plan Row also has some optional attributes:
        - a label
        - a rich text
        - a unit (depending on the type, it only makes sense for a dimension)
        - a format (attribute specific to the Timestamp type)
        A Data Collect Plan Row may have some additional information thanks to SampleSize, SampleFrequency and/or TargetValues interfaces
        NOTA: The UnitEnum type cannot be used anymore for Unit attribute as we now export the unit internal name and not its symbol (that can be empty)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ObjectBase">
        <xsd:sequence>
          <xsd:element name="CollectMode" type="DataCollectModeEnum" minOccurs="1" default="Collected"/>
          <xsd:element name="Label" type="xsd:string" minOccurs="0"/>
          <xsd:element name="RichText" type="xsd:string" minOccurs="0"/>
          <xsd:element name="NbMeasurement" type="xsd:integer" minOccurs="0" default="1"/>
          <xsd:element name="TypeUnitFormat" minOccurs="1">
            <xsd:complexType>
              <xsd:attribute name="Type" type="MagnitudeEnum" use="required"/>
              <xsd:attribute name="Unit" type="xsd:string" use="optional"/>
              <xsd:attribute name="Format" type="xsd:string" use="optional"/>
            </xsd:complexType>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="SampleSizeDouble" type="AttributeValueDoubleType"/>
            <xsd:element name="SampleSizeDiscrete" type="AttributeValueIntegerType"/>
          </xsd:choice>
          <xsd:choice minOccurs="0">
            <xsd:element name="SampleFrequencyDouble" type="AttributeValueDoubleType"/>
            <xsd:element name="SampleFrequencyInteger" type="AttributeValueIntegerType"/>
            <xsd:element name="SampleFrequencyEvent" type="AttributeValueStringType"/>
            <xsd:element name="SampleFrequencyTime" type="AttributeValueDoubleType"/>
          </xsd:choice>
          <xsd:element name="TargetValues" minOccurs="0">
            <xsd:complexType>
              <xsd:choice minOccurs="1">
                <xsd:element name="TargetValuesText" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="ValueText" type="AttributeValueStringType" maxOccurs="unbounded"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="TargetValuesBoolean" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="ValuesBoolean" type="AttributeValueStringType" minOccurs="2" maxOccurs="2"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="TargetValuesInteger" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="ValuesInteger" minOccurs="0">
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element name="ValueInteger" type="AttributeValueIntegerType" maxOccurs="unbounded"/>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="ValueIntegerFormula" type="AttributeValueStringType" minOccurs="0"/>
                      <xsd:element name="MinInteger">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueIntegerType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="MinIntegerFormula" type="AttributeValueStringType" minOccurs="0"/>
                      <xsd:element name="MaxInteger">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueIntegerType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="MaxIntegerFormula" type="AttributeValueStringType" minOccurs="0"/>
					  <xsd:element name="MinIntegerControlLimit">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueIntegerType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
					  <xsd:element name="MaxIntegerControlLimit">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueIntegerType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
                <xsd:element name="TargetValuesDouble" minOccurs="0">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="ValueDouble" type="AttributeValueDoubleType" minOccurs="0"/>
                      <xsd:element name="ValueDoubleFormula" type="AttributeValueStringType" minOccurs="0"/>
                      <xsd:element name="MinDouble">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueDoubleType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="MinDoubleFormula" type="AttributeValueStringType" minOccurs="0"/>
                      <xsd:element name="MaxDouble">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueDoubleType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="MaxDoubleFormula" type="AttributeValueStringType" minOccurs="0"/>
					  <xsd:element name="MinDoubleControlLimit">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueDoubleType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
					  <xsd:element name="MaxDoubleControlLimit">
                        <xsd:complexType>
                          <xsd:complexContent>
                            <xsd:extension base="AttributeValueDoubleType">
                              <xsd:attribute name="isIncluded" type="xsd:boolean" use="optional" default="false"/>
                            </xsd:extension>
                          </xsd:complexContent>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:choice>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="SpecifyHowToCnx" type="ObjectRef" minOccurs="0" info:typeRef="RequirementFullfillLinkType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
