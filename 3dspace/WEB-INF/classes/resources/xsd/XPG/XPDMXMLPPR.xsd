  <xsd:complexType name="ProductTransformationBase" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Defines the Transformation to achieve on a Product (i.e. Intent)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReferenceBase">
        <xsd:sequence>
          <xsd:element name="EstimatedTime" type="AttributeValueDurationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Estimated time of execution. Can be used as specification to roughly review the process flow.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ThroughputTime" type="AttributeValueDurationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time of the critical path considering the precedence/prerequisite constraints between aggregated Transformation instances.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AnalysedTime" type="AttributeValueDurationType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Time of execution calculated after time analysis.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
