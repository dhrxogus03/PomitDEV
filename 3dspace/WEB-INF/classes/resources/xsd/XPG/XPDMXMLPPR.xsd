  <xsd:group name="ProductImplementScopeRefGroup">
    <xsd:annotation>
      <xsd:documentation>
        ProductImplementScopeRef delimits a subset of the Product Structure that can be implemented by its children instanciated Transformation.
        The scope is defined by an implement link from a  Transformation reference to a Product reference
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
	<xsd:choice minOccurs="0">
      <xsd:element name="ProductImplementPartialScopeRef" type="ObjectRef" minOccurs="0" maxOccurs="unbounded" info:typeRef="Product3DType"/>
