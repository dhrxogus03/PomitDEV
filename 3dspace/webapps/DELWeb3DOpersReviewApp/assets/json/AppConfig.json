                               "allowDnD":false,
                               "contextualMenuContent":{
                                   "geometry":{
                                       "firstLevel":[
                                           "FocusOnHdr",
                                           "ReframeOnHdr",
                                           "HideShowHdr",
                                           "TransparencyHdr",
                                           "ActionBar_Attributes"
                                       ]
                                   }
                               }
                           },
                           "wkiview_opts":{
                           "panelOptions":{
                                "currentDockArea":8
                            }
                           }
                       },
                       "run_options":{
                           "activateVolumeFilter":{
                               "value":true
                           },
                           "authorized_root_types":{
                               "value":[
                                   "VPMReference",
                                   "DELFmiFunctionReference"
                               ]
                           },
                           "unsupported_root_types":{
                               "value":[
                                   "PPRContext"
                               ]
                            },
                            "propertiesFacet":{
                                "value":[
                                {
                                    "require":"DS/DELWebViewerFacets/DELWkiInstructionsFacet",
                                    "facet":{
                                        "name":"work_instructions",
                                        "icon":"fonticon fonticon-book-open"
                                    },
                                    "position":0,
                                    "alwaysVisible":false
                                }
                                ]
                            },
                            "activateWidgetLink": {
                                "value" : true
                            }
                        }
                    }
               },
               "dragAndDrop":true,
               "activateWidgetLink": true,
               "widget_link_controller_config":{
                    "matchKeys":["SBOM","MBOM","PRODUCT","RESOURCE"],
                    "restrictKeys":["PROCESS"],
                    "shareContent":true
                }
           }
       },
       "LandingPage":{
           "code":"DS/DELWebLandingPage/DELWebLandingPage",
