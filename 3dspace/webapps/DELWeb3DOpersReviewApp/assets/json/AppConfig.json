{
   "Components":{
       "Viewer":{
           "code":"DS/DELWeb3DViewerComp/DELWeb3DViewer",
           "options":{
               "viewerType": 2,
               "quickToolBar": true,
               "removeMaterial": true,
               "statusBar": {
                  "commands":["LastIndexingTime", "StartUpView"]
               }, 
               "sharedSettings":[
                    "synthesisModeOnInstance", 
                    "enopad3dviewer_CGRorTHB", 
                    "enopad3dviewer_displayCandidateQueue",
                    "enopad3dviewer_displayCandidateQueue_color", 
                    "pgpFromServer",
                    "pad_security_ctx"
               ],
               "showLevelSelectorLabel":true,
               "defaultViewerMode":"WorkInstructions",
               "defaultPickability":"SHAPE",
               "requirementFTA": true,
               "persistBuildUpSettings":true,
               "viewerOptions":{
                   "delviewer":{
                       "layout_options":{
                           "frmWindow_opts":{
                            "viewerOptions":{
                                "highlight": {
                                   "trapGPU": true
                                }
                             },
                               "workbench":"DELWeb3DOpersReviewApp.xml",
