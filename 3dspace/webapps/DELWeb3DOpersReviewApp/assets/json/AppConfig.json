                        }
                    },
                    "aggregation_processors": [
                        {
                            "truncate": {
                                "max_distance_from_prefix": 1,
                                "match_only_refs": 1,
                                "sequence_filter": {
                                    "sequence": [
                                        {
                                            "uql": "(flattenedtaxonomies:types/DELFmiFunctionReference)"
                                        },
                                        {
                                            "statement": "END"
                                        }
                                    ]
