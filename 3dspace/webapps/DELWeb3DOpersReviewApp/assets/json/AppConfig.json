                       "dataIndex":"ds6w:label.reference"
                   },
                   "Columns":[
                       {
                           "name":"InstanceLabel",
                           "dicoType":"DELFmiFunctionIdentifiedInstance",
                           "dataIndex":"ds6w:label.instance"
                       },
                       {
                          "name":"Scope",
                          "dataIndex":"to.DELFmiProcessImplementCnx"
                       },
                       {
