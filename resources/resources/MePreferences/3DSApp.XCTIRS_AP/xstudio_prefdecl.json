{
  "repositories": [
    {
      "name": "xStudioApp",
      "preferences": [
        {
          "name": "autoLPR",
          "datatype": "boolean",
          "default": true,
          "deprecated": false
        },
        {
          "name": "aspectRatioPreset",
          "datatype": "enum",
          "values": ["Web", "Ecommerce", "SocialMedia", "AutoFit", "Custom"],
