<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.3ds.com/xsd/XPDMXML" xmlns:dns="http://www.3ds.com/xsd/XPDMXML" targetNamespace="http://www.3ds.com/xsd/XPDMXML" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="XPDMXMLCore.xsd"/>
	<!-- Column Type definition -->
	<xsd:simpleType name="PressureColumn">
		<xsd:annotation>
			<xsd:documentation> Data column with units same as pressure
			</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="PressureType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TemperatureColumn">
		<xsd:annotation>
			<xsd:documentation> Temperature data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="TemperatureType"/>
	</xsd:simpleType>
	<xsd:simpleType name="DimensionlessColumn">
		<xsd:annotation>
			<xsd:documentation> Dimensionless data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="ScalarType"/>
	</xsd:simpleType>
	<xsd:simpleType name="FrequencyColumn">
		<xsd:annotation>
			<xsd:documentation>Frequency data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="FrequencyType"/>
	</xsd:simpleType>
	<xsd:simpleType name="ThermalConductivityColumn">
		<xsd:annotation>
			<xsd:documentation>Thermal Conductivity data column
			</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="ThermalConductivityType"/>
	</xsd:simpleType>
	<xsd:simpleType name="DensityColumn">
		<xsd:annotation>
			<xsd:documentation>Density data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="DensityType"/>
	</xsd:simpleType>
	<xsd:simpleType name="InverseTemperatureColumn">
		<xsd:annotation>
			<xsd:documentation>Inverse Temperature data column
			</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="InverseTemperatureType"/>
	</xsd:simpleType>
	<xsd:simpleType name="CompressibilityColumn">
		<xsd:annotation>
			<xsd:documentation>Compressibility data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="CompressibilityType"/>
	</xsd:simpleType>
	<xsd:simpleType name="EnergyColumn">
		<xsd:annotation>
			<xsd:documentation>Energy data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="EnergyType"/>
	</xsd:simpleType>
	<xsd:simpleType name="MolarHeatCapacityColumn">
		<xsd:annotation>
			<xsd:documentation>Molar heat capacity data column
			</xsd:documentation>
		</xsd:annotation>
		<!-- Has to be replace with molar heat capacity type -->
		<xsd:list itemType="ScalarType"/>
	</xsd:simpleType>
	<xsd:simpleType name="RStiffnessColumn">
		<xsd:annotation>
			<xsd:documentation>RStiffness data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="RStiffnessType"/>
	</xsd:simpleType>
	<xsd:simpleType name="LengthColumn">
		<xsd:annotation>
			<xsd:documentation>Length data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="LengthType"/>
	</xsd:simpleType>
	<xsd:simpleType name="SpecificHeatCapacitylinearPartColumn">
		<xsd:annotation>
			<xsd:documentation>SpecificHeatCapacitylinearPart data column
			</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="SpecificHeatCapacityLinearPartType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TStiffnessColumn">
		<xsd:annotation>
			<xsd:documentation>TStiffness data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="TStiffnessType"/>
	</xsd:simpleType>
	<xsd:simpleType name="AngleColumn">
		<xsd:annotation>
			<xsd:documentation>Angle data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="AngleType"/>
	</xsd:simpleType>
	<xsd:simpleType name="DynamicViscosityColumn">
		<xsd:annotation>
			<xsd:documentation>DynamicViscosity data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="DynamicViscosityType"/>
	</xsd:simpleType>
	<xsd:simpleType name="TimeColumn">
		<xsd:annotation>
			<xsd:documentation>Time data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="TimeType"/>
	</xsd:simpleType>
	<xsd:simpleType name="StrainRateColumn">
		<xsd:annotation>
			<xsd:documentation>StrainRate data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="StrainRateType"/>
	</xsd:simpleType>
	<xsd:simpleType name="SpecificEnergyColumn">
		<xsd:annotation>
			<xsd:documentation>SpecificEnergy data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="SpecificEnergyType"/>
	</xsd:simpleType>
	<xsd:simpleType name="AcousticImpedanceColumn">
		<xsd:annotation>
			<xsd:documentation>AcousticImpedance data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="AcousticImpedanceType"/>
	</xsd:simpleType>
	<xsd:simpleType name="FlowResistivityColumn">
		<xsd:annotation>
			<xsd:documentation>FlowResistivity data column </xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="FlowResistivityType"/>
	</xsd:simpleType>
	<xsd:complexType name="FrequencyTabularIsotropicNoneTableType">
		<xsd:sequence>
			<xsd:element name="Omegagreal" type="DimensionlessColumn"/>
			<xsd:element name="Omegagimag" type="DimensionlessColumn"/>
			<xsd:element name="Omegakreal" type="DimensionlessColumn"/>
			<xsd:element name="Omegakimag" type="DimensionlessColumn"/>
			<xsd:element name="Frequency" type="FrequencyColumn"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="KinematicTableType">
		<xsd:sequence>
			<xsd:element name="YieldStress" type="PressureColumn"/>
			<xsd:sequence minOccurs="1" maxOccurs="10">
				<xsd:element name="C_n" type="PressureColumn"/>
				<xsd:element name="Gamma_n" type="DimensionlessColumn"/>
			</xsd:sequence>
			<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RateJohnsonCookDataType">
		<xsd:sequence>
			<xsd:element name="C" type="AttributeValueDoubleType" default="0.0"/>
			<!-- The below should be changed to frequency type after definiting the 
				data type -->
			<xsd:element name="EpsilonDotZero" type="AttributeValueDoubleType" default="0.0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Simulation Domain Options -->
	<xsd:complexType name="SimulationOptionBase" abstract="true">
		<xsd:attribute name="id" type="xsd:ID" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="CastIronPlasticityType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for CastIronPlasticity material
					option.
					Add further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:element name="CompressionTable">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="SigmaC" type="PressureColumn"/>
								<xsd:element name="EpsilonC" type="DimensionlessColumn"/>
								<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="PlasicityTable">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="PlasticPoissonsRatio" type="DimensionlessColumn"/>
								<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="TensionTable">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="SigmaT" type="PressureColumn"/>
								<xsd:element name="EpsilonT" type="DimensionlessColumn"/>
								<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="CastIronPlasticity" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConductivityType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for IsotropicConductivity material
					option.
					Add further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:choice>
					<xsd:element name="IsotropicTable">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="ThermalConductivity" type="ThermalConductivityColumn"/>
								<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:choice>
				<xsd:attribute name="optionType" type="xsd:string" fixed="Conductivity" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CreepType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for Creep material option.
					Add
					further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="StrainHardeningTable">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="PowerLawMultiplier" type="DimensionlessColumn"/>
									<xsd:element name="EquivalentStressOrder" type="DimensionlessColumn"/>
									<xsd:element name="TimeOrder" type="DimensionlessColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="TimeHardeningTable">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="PowerLawMultiplier" type="DimensionlessColumn"/>
									<xsd:element name="EquivalentStressOrder" type="DimensionlessColumn"/>
									<xsd:element name="TimeOrder" type="DimensionlessColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="HyperbolicSineTable">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="PowerLawMultiplier" type="FrequencyColumn"/>
									<xsd:element name="HyperbolicLawMultiplier" type="CompressibilityColumn"/>
									<xsd:element name="EquivalentStressOrder" type="DimensionlessColumn"/>
									<xsd:element name="ActivationEnergy" type="EnergyColumn"/>
									<xsd:element name="UniversalGasConstant" type="MolarHeatCapacityColumn"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:choice>
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="Creep" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DampingType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for Damping material option.
					Add
					further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:element name="Alpha" type="AttributeValueDoubleType" default="0.0"/>
					<xsd:element name="Beta" type="AttributeValueDoubleType" default="0.0"/>
					<xsd:element name="Structural" type="AttributeValueDoubleType" default="0.0"/>
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="Damping" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ElasticType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is the schema for elastic material option.
					Types of elasticity :
					1) Isotropic,
					2) Orthotrophic,
					3) Engineering
					constants,
					4) Lamina,
					5) Anistropic

					Moduli time scale option is
					applicable when elastic is used
					in conjunction with the viscoelastic
					option.
					Longtime- Elastic constant defines long term behavior
					Instantaneous - Elastic constant defines instantaneous behavior
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="Isotropic">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="YoungsModulus" type="PressureColumn"/>
									<xsd:element name="PoissonsRatio" type="DimensionlessColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Orthotropic">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="D1111" type="PressureColumn"/>
									<xsd:element name="D1122" type="PressureColumn"/>
									<xsd:element name="D2222" type="PressureColumn"/>
									<xsd:element name="D1133" type="PressureColumn"/>
									<xsd:element name="D2233" type="PressureColumn"/>
									<xsd:element name="D3333" type="PressureColumn"/>
									<xsd:element name="D1212" type="PressureColumn"/>
									<xsd:element name="D1313" type="PressureColumn"/>
									<xsd:element name="D2323" type="PressureColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="EngineeringConstant">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="E1" type="PressureColumn"/>
									<xsd:element name="E2" type="PressureColumn"/>
									<xsd:element name="E3" type="PressureColumn"/>
									<xsd:element name="Nu12" type="DimensionlessColumn"/>
									<xsd:element name="Nu13" type="DimensionlessColumn"/>
									<xsd:element name="Nu23" type="DimensionlessColumn"/>
									<xsd:element name="G12" type="PressureColumn"/>
									<xsd:element name="G13" type="PressureColumn"/>
									<xsd:element name="G23" type="PressureColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Lamina">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="E1" type="PressureColumn"/>
									<xsd:element name="E2" type="PressureColumn"/>
									<xsd:element name="Nu12" type="DimensionlessColumn"/>
									<xsd:element name="G12" type="PressureColumn"/>
									<xsd:element name="G13" type="PressureColumn"/>
									<xsd:element name="G23" type="PressureColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Anisotropic">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="D1111" type="PressureColumn"/>
									<xsd:element name="D1122" type="PressureColumn"/>
									<xsd:element name="D2222" type="PressureColumn"/>
									<xsd:element name="D1133" type="PressureColumn"/>
									<xsd:element name="D2233" type="PressureColumn"/>
									<xsd:element name="D3333" type="PressureColumn"/>
									<xsd:element name="D1112" type="PressureColumn"/>
									<xsd:element name="D2212" type="PressureColumn"/>
									<xsd:element name="D3312" type="PressureColumn"/>
									<xsd:element name="D1212" type="PressureColumn"/>
									<xsd:element name="D1113" type="PressureColumn"/>
									<xsd:element name="D2213" type="PressureColumn"/>
									<xsd:element name="D3313" type="PressureColumn"/>
									<xsd:element name="D1213" type="PressureColumn"/>
									<xsd:element name="D1313" type="PressureColumn"/>
									<xsd:element name="D1123" type="PressureColumn"/>
									<xsd:element name="D2223" type="PressureColumn"/>
									<xsd:element name="D3323" type="PressureColumn"/>
									<xsd:element name="D1223" type="PressureColumn"/>
									<xsd:element name="D1323" type="PressureColumn"/>
									<xsd:element name="D2323" type="PressureColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="ModuliTimeScale" default="LongTerm" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="LongTerm"/>
								<xsd:enumeration value="Instantaneous"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="Elasticity" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExpansionType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for Expansion material option.
					Add
					further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="IsotropicTable">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ExpansionCoeffalpha" type="InverseTemperatureColumn"/>
									<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="ReferenceTemp" type="TemperatureType" default="0.0"/>
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="Expansion" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FailStrainType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for FailStrain material option.
					Add
					further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:element name="StrainFiberT" type="DimensionlessColumn"/>
					<xsd:element name="StrainFiberC" type="DimensionlessColumn"/>
					<xsd:element name="StrainTransverseT" type="DimensionlessColumn"/>
					<xsd:element name="StrainTransverseC" type="DimensionlessColumn"/>
					<xsd:element name="ShearStrain" type="DimensionlessColumn"/>
					<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="FailStrain" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FailStressType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for FailStress material option.
					Add
					further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:element name="StressFiberT" type="PressureColumn"/>
					<xsd:element name="StressFiberC" type="PressureColumn"/>
					<xsd:element name="StressTransverseT" type="PressureColumn"/>
					<xsd:element name="StressTransverseC" type="PressureColumn"/>
					<xsd:element name="ShearStrength" type="PressureColumn"/>
					<xsd:element name="CrossProductTermCoefficient" type="DimensionlessColumn"/>
					<xsd:element name="EquibiaxialStressLimit" type="PressureColumn"/>
					<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
				</xsd:sequence>
				<xsd:attribute name="optionType" type="xsd:string" fixed="FailStress" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="HyperelasticityType" >
		<xsd:complexContent>
			<xsd:annotation>
				<xsd:documentation>
					This is schema for Hyperelasticity material
					option.
					Add further documentation here.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:extension base="SimulationOptionBase">
				<xsd:sequence>
					<xsd:choice>
						<xsd:element name="Isotropic">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:choice>
										<xsd:element name="ArrudaBoyceTable">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="mu" type="PressureColumn"/>
													<xsd:element name="lambdam" type="DimensionlessColumn"/>
													<xsd:element name="D" type="CompressibilityColumn"/>
													<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="NeoHookeTable">
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="C10" type="PressureColumn"/>
													<xsd:element name="D1" type="CompressibilityColumn"/>
													<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:choice>
											<xsd:element name="OgdenTable1">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="mu1" type="PressureColumn"/>
														<xsd:element name="alpha1" type="DimensionlessColumn"/>
														<xsd:element name="D1" type="CompressibilityColumn"/>
														<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="OgdenTable2">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="mu1" type="PressureColumn"/>
														<xsd:element name="alpha1" type="DimensionlessColumn"/>
														<xsd:element name="mu2" type="PressureColumn"/>
														<xsd:element name="alpha2" type="DimensionlessColumn"/>
														<xsd:element name="D1" type="CompressibilityColumn"/>
														<xsd:element name="D2" type="CompressibilityColumn"/>
														<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="OgdenTable3">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="mu1" type="PressureColumn"/>
														<xsd:element name="alpha1" type="DimensionlessColumn"/>
														<xsd:element name="mu2" type="PressureColumn"/>
														<xsd:element name="alpha2" type="DimensionlessColumn"/>
														<xsd:element name="mu3" type="PressureColumn"/>
														<xsd:element name="alpha3" type="DimensionlessColumn"/>
														<xsd:element name="D1" type="CompressibilityColumn"/>
														<xsd:element name="D2" type="CompressibilityColumn"/>
														<xsd:element name="D3" type="CompressibilityColumn"/>
														<xsd:element name="Temperature" type="TemperatureColumn" minOccurs="0" />
													</xsd:sequence>
												</xsd:complexType>
											</xsd:element>
											<xsd:element name="OgdenTable4">
												<xsd:complexType>
													<xsd:sequence>
														<xsd:element name="mu1" type="PressureColumn"/>
														<xsd:element name="alpha1" type="DimensionlessColumn"/>
														<xsd:element name="mu2" type="PressureColumn"/>
														<xsd:element name="alpha2" type="DimensionlessColumn"/>
														<xsd:element name="mu3" type="PressureColumn"/>
														<xsd:element name="alpha3" type="DimensionlessColumn"/>
